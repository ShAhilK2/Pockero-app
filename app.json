{
  "expo": {
    "name": "Pocktica",
    "slug": "Pocktica",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/pocktica.png",
    "scheme": "pocktica",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.pocktica",
      "infoPlist": {
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": [
              "pocktica"
            ]
          }
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/pocktica.png",
        "backgroundImage": "./assets/images/pocktica.png",
        "monochromeImage": "./assets/images/pocktica.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.anonymous.pocktica",
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "*.clerk.accounts.dev"
            },
            {
              "scheme": "https",
              "host": "clerk.pocktica.com"
            },
            {
              "scheme": "pocktica"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "output": "server",
      "favicon": "./assets/images/pocktica.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/pocktica.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "./plugins/withAndroidDrawables.js",
      "expo-sqlite",
      [
        "@sentry/react-native/expo",
        {
          "url": "https://sentry.io/",
          "project": "pocktica-app",
          "organization": "kataria-ki"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
